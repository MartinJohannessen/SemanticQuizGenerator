SELECT ?hovedstad ?hovedstadLabel ?kulturminnestatus ?kulturminnestatusLabel WHERE {
  SERVICE wikibase:label { bd:serviceParam wikibase:language "[AUTO_LANGUAGE],en". }
  
  
  
  ?hovedstad wdt:P31 wd:Q5119.
  
  ?hovedstad wdt:P1435 wd:Q9259.
  OPTIONAL { ?hovedstad wdt:P2614 ?kulturminnestatus. }
}
LIMIT 100
